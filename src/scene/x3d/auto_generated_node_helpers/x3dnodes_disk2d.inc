   FdOuterRadius.ChangeAlways := chVisibleNonGeometry;
   FdOuterRadius.MustBeNonnegative := true;
  AddField(FFdOuterRadius);

  FFdSolid := TSFBool.Create(Self, False, 'solid', false);
   FdSolid.ChangeAlways := chVisibleNonGeometry;
  AddField(FFdSolid);
end;

