{ TComposedVolumeStyleNode ----------------------------------------------- }

procedure TComposedVolumeStyleNode.SetRenderStyle(const Value: array of TAbstractComposableVolumeRenderStyleNode);
var
  L: Integer;
  A: array of TX3DNode;
begin
  L := High(Value) + 1;
  SetLength(A, L);
  if L > 0 then
    Move(Value[0], A[0], L * SizeOf(TAbstractComposableVolumeRenderStyleNode));
  FdRenderStyle.Send(A);
end;

class function TComposedVolumeStyleNode.ClassX3DType: String;
begin
  Result := 'ComposedVolumeStyle';
end;

procedure TComposedVolumeStyleNode.CreateNode;
begin
  inherited;

  FFdRenderStyle := TMFNode.Create(Self, True, 'renderStyle', [TAbstractComposableVolumeRenderStyleNode]);
   FdRenderStyle.ChangeAlways := chVisibleNonGeometry;
  AddField(FFdRenderStyle);
end;

